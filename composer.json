{
  "name": "mckenzie-org/crux-redis",
  "description": "Crux Redis Module.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Akshay Kolte",
      "email": "akshay.kolte@etlok.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Etlok\\Crux\\Redis\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Etlok\\Crux\\Redis\\Tests\\": "tests"
