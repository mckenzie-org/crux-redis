    }
  },
"repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/mckenzie-org/crux"
    },
    {
        "type": "vcs",
        "url": "https://github.com/mckenzie-org/crux-redis"
    },
    {
        "type": "vcs",
        "url": "https://github.com/mckenzie-org/crux-websockets"
    }
],   "require": {
    "mckenzie-org/crux": "1.*",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.6"
  }
}
